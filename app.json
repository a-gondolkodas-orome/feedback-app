{
  "expo": {
    "name": "Study Meter",
    "slug": "study-meter",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/kisfej512.png",
    "splash": {
      "image": "./assets/kisfej512_transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b1633"
    },
    "primaryColor": "#0b1633",
    "notification": {
      "icon": "./assets/kisfej96bw.png",
      "color": "#0b1633"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "hu.agondolkodasorome.studymeter",
      "supportsTablet": true
    },
    "android": {
      "package": "hu.agondolkodasorome.studymeter",
      "permissions": [
        "VIBRATE"
      ],
      "googleServicesFile": "./google-services.json"
    }
  }
}
