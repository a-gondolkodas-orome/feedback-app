{
  "expo": {
    "name": "Feedback app",
    "slug": "feedback-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/kisfej.png",
    "splash": {
      "image": "./assets/kisfej.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b1633"
    },
    "primaryColor": "#0b1633",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "hu.agondolkodasorome.feedback",
      "supportsTablet": true
    },
    "android": {
      "package": "hu.agondolkodasorome.feedback"
    }
  }
}
